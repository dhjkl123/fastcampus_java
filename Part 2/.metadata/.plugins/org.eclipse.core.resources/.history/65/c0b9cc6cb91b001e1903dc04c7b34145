package ch04;

import java.util.ArrayList;

public class CustomerTest {

	public static void main(String[] args) {
		ArrayList<Customer> customerList = new ArrayList<>();
		Customer customer1 = new Customer(10010, "Tomas");
		Customer customer2 = new Customer(10020, "James");
		Customer customer3 = new Customer(10030, "Edward");
		Customer customer4 = new Customer(10040, "Percy");
		Customer customer5 = new Customer(10050, "Kim");
		
		customerList.add(customer1);
		customerList.add(customer2);
		customerList.add(customer3);
		customerList.add(customer4);
		customerList.add(customer5);
		
		for (Customer customer : customerList) {
			System.out.println(customer.showCustomerInfo())
		}

		
	}

}
