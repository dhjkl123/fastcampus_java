package ch13;

import java.io.IOException;

public class SystemInTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("알파벳 여러 개를 쓰고 [Enter]를 누르세요");
		
		int i = 0;
		
		try {
			i = System.in.read();
			System.out.println(i);
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}
