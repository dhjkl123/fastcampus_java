package school;

public class Student {
	public String Name;
	private String Number;
	private Score Korean;
	private Score Math;
	
	public Student() {}
	
	public Student(String Name, String Number, Score Korean, Score Math) {
		this.Name = Name;
		this.Number = Number;
		this.Korean = Korean;
		this.Math = Math;
	}
	
	public String getName() {
		return Name;
	}
	public void setName(String name) {
		this.Name = name;
	}
	public String getNumber() {
		return Number;
	}
	public void setNumber(String number) {
		Number = number;
	}
	public Score getKorean() {
		return Korean;
	}
	public void setKorean(Score korean) {
		Korean = korean;
	}
	public Score getMath() {
		return Math;
	}
	public void setMath(Score math) {
		Math = math;
	}
	
	
}
